{
  "version": 2,
  "builds": [
    {
      "src": "clients/line_bot/line_bot_v5_clean.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "clients/line_bot/line_bot_v5_clean.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "clients/line_bot/line_bot_v5_clean.py": {
      "maxDuration": 30
    }
  }
}